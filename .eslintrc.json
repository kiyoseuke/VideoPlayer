{
  "env": {
    "browser": true,
    "node": true
  },
  "extends": [
    "eslint:recommended"
  ],
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "rules": {
    "no-console": [
      "warn"
    ],
    "no-irregular-whitespace": [
      "error"
    ],
    "no-undef": "off",
    "indent": [
      "error",
      2
    ],
    "semi": "error"
  }
}